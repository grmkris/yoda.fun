{
  "name": "@yoda.fun/api",
  "exports": {
    ".": {
      "types": "./dist/api.d.ts",
      "default": "./src/api.ts"
    },
    "./routers": {
      "types": "./dist/routers/routers.d.ts",
      "default": "./src/routers/routers.ts"
    },
    "./context": {
      "types": "./dist/context.d.ts",
      "default": "./src/context.ts"
    }
  },
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "lint": "bun ultracite check",
    "fix": "bun ultracite fix",
    "fix:unsafe": "bun ultracite fix --unsafe",
    "typecheck": "tsc --noEmit",
    "test": "bun test"
  },
  "devDependencies": {
    "tsdown": "catalog:"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@orpc/server": "catalog:",
    "@orpc/client": "catalog:",
    "@orpc/openapi": "catalog:",
    "@orpc/zod": "catalog:",
    "hono": "catalog:",
    "zod": "catalog:",
    "@yoda.fun/auth": "workspace:*",
    "@yoda.fun/db": "workspace:*",
    "@yoda.fun/shared": "workspace:*"
  }
}
