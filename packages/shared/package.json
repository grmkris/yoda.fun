{
  "name": "@yoda.fun/shared",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./services": {
      "types": "./dist/services.schema.d.ts",
      "default": "./src/services.schema.ts"
    },
    "./typeid": {
      "types": "./dist/typeid.schema.d.ts",
      "default": "./src/typeid.schema.ts"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "default": "./src/constants.ts"
    },
    "./resolution-types": {
      "types": "./dist/resolution-types.d.ts",
      "default": "./src/resolution-types.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "lint": "bun ultracite check",
    "fix": "bun ultracite fix",
    "fix:unsafe": "bun ultracite fix --unsafe",
    "typecheck": "tsc --noEmit",
    "test": "bun test"
  },
  "devDependencies": {
    "tsdown": "catalog:"
  },
  "dependencies": {
    "typeid-js": "catalog:",
    "zod": "catalog:"
  }
}
